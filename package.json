{
  "name": "reporting-tool-backend",
  "version": "1.0.0",
  "description": "Node.js boilerplate using fastify & TypeScript",
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc -p .",
    "start": "node -r tsconfig-paths/register -r ts-node/register ./build | pino-pretty --colorize",
    "dev": "nodemon | pino-pretty --colorize",
    "pretest": "npm run build",
    "test": "cross-env NODE_ENV=test jest --config .jest.json --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --config .jest.json --watch --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calixtoyg/reporting-tool-backend"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "nodejs",
    "fastify",
    "typescript"
  ],
  "author": "Yonatan Bendahan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yonathan06/fastify-typescript-boilerplate/issues"
  },
  "homepage": "https://github.com/yonathan06/fastify-typescript-boilerplate#readme",
  "devDependencies": {
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.16",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.6.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "26.6.3",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.2.1",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^3.9.9",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "csvtojson": "^2.0.10",
    "env-schema": "^2.0.1",
    "fastify": "^3.9.2",
    "fastify-cors": "^6.0.1",
    "fastify-multipart": "^4.0.6",
    "fastify-now": "^2.3.1",
    "fastify-swagger": "^4.7.0",
    "fluent-json-schema": "^2.0.3",
    "http-errors": "^1.8.0",
    "lodash": "^4.17.21"
  }
}
